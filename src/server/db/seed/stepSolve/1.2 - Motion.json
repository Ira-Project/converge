                    ],
                    "options": [
                        {
                            "optionText": "$!$\\text{average speed} = \\frac{\\text{total distance travelled}}{\\text{total time taken}}$!$",
                            "optionImage": "",
                            "isCorrect": true
                        },
                        {
                            "optionText": "$!$\\text{average speed} = \\text{total distance travelled} \\times \\text{total time taken}$!$",
